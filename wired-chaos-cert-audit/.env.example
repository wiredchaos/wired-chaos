# Project DB (certificate source + audit sink)
DATABASE_URL=postgresql://user:pass@localhost:5432/wiredchaos

# Preferred private DOGE inscription indexer
DOGE_INDEXER_BASE=https://indexer.your-doge-node.local
DOGE_INDEXER_KEY=xxxx

# Fallback read-only explorers / marketplaces (read-only, optional)
EXPLORER_BASE=https://doge.explorer.api
EXPLORER_KEY=yyyy

MARKET_BASE=https://doginals.market.api
MARKET_KEY=zzzz

# Optional RPC relay for raw tx lookups (no inscription parsing)
DOGE_RPC_BASE=https://rpc-relay.api
DOGE_RPC_KEY=rrrr

# Service
PORT=8080
CACHE_DIR=.cache
LOG_LEVEL=info
