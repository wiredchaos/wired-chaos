{
  "name": "@wired-chaos/wix-gamma-integration",
  "version": "1.0.0",
  "description": "WIX & GAMMA Integration System for WIRED CHAOS",
  "main": "cloudflare/workers/integration-worker.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev cloudflare/workers/integration-worker.js",
    "deploy": "wrangler deploy cloudflare/workers/integration-worker.js --env production",
    "deploy:staging": "wrangler deploy cloudflare/workers/integration-worker.js --env staging",
    "deploy:dev": "wrangler deploy cloudflare/workers/integration-worker.js --env development",
    "tail": "wrangler tail --env production",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "echo 'Unit tests not yet implemented'",
    "test:integration": "echo 'Integration tests not yet implemented'",
    "format": "prettier --write '**/*.{js,ts,json,md}'",
    "lint": "eslint '**/*.{js,ts}'",
    "typecheck": "tsc --noEmit",
    "kv:create": "npm run kv:create:cache && npm run kv:create:analytics && npm run kv:create:sync",
    "kv:create:cache": "wrangler kv:namespace create CACHE_KV",
    "kv:create:analytics": "wrangler kv:namespace create ANALYTICS_KV",
    "kv:create:sync": "wrangler kv:namespace create SYNC_KV",
    "r2:create": "wrangler r2 bucket create wired-chaos-ar-models",
    "setup": "npm install && npm run kv:create && npm run r2:create"
  },
  "keywords": [
    "wix",
    "gamma",
    "cloudflare",
    "workers",
    "integration",
    "wired-chaos",
    "ar",
    "vr",
    "presentations"
  ],
  "author": "WIRED CHAOS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wiredchaos/wired-chaos.git",
    "directory": "wix-gamma-integration"
  },
  "bugs": {
    "url": "https://github.com/wiredchaos/wired-chaos/issues"
  },
  "homepage": "https://github.com/wiredchaos/wired-chaos/tree/main/wix-gamma-integration#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "wrangler": "^3.78.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
